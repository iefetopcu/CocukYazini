@using CocukYazini.Models.Entity
@{
    ViewBag.Title = "Çocuk Yazını TV";
    ViewBag.Description = "Çocuk yazını video listesi";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    iframe{
        width:100%!important;
    }
</style>
<div class="container">
    <div class="row">
        @foreach (var item in Model)
        {
            <div class="col-lg-6 text-center" style="min-height:480px;">
                <h4 style="margin-top:20px; min-height:40px;"><b>@item.videotitle</b></h4>
                @Html.Raw(item.videourl)
                <h5 style="margin-top:10px;">@item.videodescription</h5>
            </div>

        }


    </div>
</div>


